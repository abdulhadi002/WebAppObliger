    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "nodemon": "^3.0.0",
    "sass": "^1.79.4",
    "tsx": "^3.12.7",
